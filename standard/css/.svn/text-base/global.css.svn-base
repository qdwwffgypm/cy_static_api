*{padding:0;margin: 0;-webkit-box-sizing:border-box;box-sizing:border-box;outline:0;-webkit-tap-highlight-color:transparent;-webkit-tap-highlight-color:transparent}
body{font-family:'Consolas';font-size:14px;line-height: 20px;color: #444;background-color:#fff;-webkit-overflow-scrolling:touch;padding-bottom: 10px;}
article,aside,details,figcaption,figure,footer,header,hgroup,main,nav,section,summary{display:block}
audio,canvas,progress,video{display:inline-block;vertical-align:baseline}
audio:not([controls]){display:none;height:0}
table{border-spacing:0;border-collapse:collapse}
[hidden],template{display:none}
a{background:0 0;color:#1aa0ff;}
a:active,a:hover{outline:0}
a:active{color:#0062cc}
ul, ol {list-style: none;}
a {text-decoration: none;color:#666;}
a:hover { color:#666; text-decoration: underline; }
abbr[title]{border-bottom:1px dotted}
b,strong{font-weight:700}
img {border: 0}
.clearfix:after, .clearfix:before{ display:table; content:' ' }
.clearfix:after{ clear:both }
.left{float:left}
.right{float:right}
.br{margin-top:10px}
.link {color: #1aa0ff;}
.lthrough{text-decoration: line-through;}
.lthroughdes{position: absolute;left: -100px;color: #999;}
header {display:block;height: 80px;}
header .middle{color:rgb(102,102,102);font-family: Consolas;font-size:18px;background-color: #fff;text-align: center;padding-top: 15px;left: 0;right: 0;z-index: 3;}

.menu{float:left;width: 260px;padding: 10px 10px 0 10px;min-height: 50px;z-index: 1;position: relative;}
.menu .middle{width: 240px;}
.menuCon{overflow: hidden;border: solid 1px #e7e7e7;padding: 0px 0px 10px 0px;position: relative;margin-right: 10px;min-height: 585px;border-top: none;margin-top: 10px;}
.helptree {border: solid 1px #dadada;background-color:#f6f6f6;margin-bottom:10px;border-bottom:none;}
.helptree h3 { height:33px; line-height:32px; color:#333; background:url(../img/titleBg.jpg) repeat-x 0 0; padding:0 10px; font-weight:bold; font-size:14px; }
.helptree ul {}
.helptree ul li {height: 30px;}
.helptree ul li a {border-bottom: 1px dashed #b9b9b9;color:#666;display:block;height: 30px;line-height: 30px;padding:0 10px;text-decoration:none;}
.helptree ul li:last-child a{border-bottom: 1px solid #dadada;}
.helptree ul li:hover { background-color:#eee; }
.helptree ul li.on {background-color: #e8e8e8;}
.helptree ul li.on a { font-weight:bold; }
.helptree ul li.on a span { font-weight:normal; }
.helptree:nth-child(2) ul li:first-child a{text-decoration: line-through;}
.listmod h3 {border-top: solid 1px #ececec;border-bottom: solid 1px #ececec;height:33px;line-height:32px;color:#333;background:url(../img/titleBg.jpg) repeat-x 0 0;padding:0 10px;font-weight:bold;font-size:14px;margin-bottom:10px;background: #fcfcfc;font-size: 12px;margin-top: 10px;}
.listmod h3:first-child {margin-top:0}
.listmod p  {padding:5px 10px;line-height: 20px;color:#666;}

.navbg:after{content:" ";background-image: url(../img/bg.png);background-repeat: no-repeat;background-position: 0 -101px;display: block;width: 15px;height: 8px;position: absolute;bottom: -6px;left: 50%;margin-left: 8px;}
.navbg {min-width: 900px;margin: 0 auto;background-image: url(../img/bg.png);background-repeat: repeat-x;background-position: 0 -49px;position: relative;}
.nav{height: 45px;padding: 0;line-height: 45px;text-align:center;text-shadow:1px 1px #fff;}
.nav,.nav li{font-size:0;letter-spacing:normal;*word-spacing:-1px;}
.nav li,.nav li a{display:inline-block;*display:inline;*zoom:1;height: 45px;}
.nav li{position: relative;}
.nav li:after{content:" ";background-image: url(../img/bg.png);background-repeat: no-repeat;background-position: -24px -101px;display: block;width: 2px;height: 15px;position: absolute;right: 0;top: 50%;margin-top: -8px;}
.nav li:last-child:after{display:none;}
.nav li a{padding:0 20px;font-weight:bold;font-size:14px;letter-spacing:normal;word-spacing:normal;color:#556165;}
.nav li a:hover{color:#333;}
.nav li i{display:none;position:absolute;left:50%;top:35px;width:14px;height:8px;margin-left:-6px;overflow:hidden;background-position:0 -66px;}

.methods{margin-bottom: 10px;position: relative;margin: 0 10px;}
ul.methods:before,
ul.methods:after{content:' ';position: absolute;background-color: #f9f9f9;width: 14px;top: 0;bottom: 0;left: 0;z-index: 1;overflow: hidden;}
ul.methods:after{right: 0;left: auto;}
.methods li{background-color: #f9f9f9;margin: 0px 10px;padding: 6px 0;border-bottom: 1px dashed #b9b9b9;position: relative;}
.methods li:last-child{border-bottom: none;}
.methods p{padding:0px 10px;line-height:18px}
.methods a{color:#1aa0ff}
.methods .explain {}
.methods .explain .title{position:absolute;top:16px;right:29px;font-weight:bold;color:#06c;padding-left:16px}
.methods .explain .title:before{content:" ";color:transparent;border-top:2px solid #06c;border-right:2px solid #06c;transform:rotate(135deg);width:8px;height:8px;position:absolute;left:0;top: 3px;}
.methods .explain .detail{display:none;padding: 20px 10px 10px 10px;}
.methods .explain.isShow .title:before {transform: rotate(315deg);top: 7px;}
.methods .explain.isShow .detail {display:block;}
.methods .explain .expList{overflow:hidden;width:954px}
.methods .explain .expList dd{width:31%;float:left;height: 405px;overflow:hidden;margin-right:10px;}
.methods .explain .expList dd pre{margin-left:-556px;line-height:21px}
.propertys{border: 1px solid #ebebeb;min-width:600px;}
.propertys th{border: 1px solid #e8e8e8;background-color: #efefef;padding: 3px 20px;}
.propertys td{border: 1px dashed #e8e8e8;padding: 2px 20px;}

.prettyprint {background-color: #efefef;padding: 5px 10px;}
.tag{color: #0066cc;}
.atn{color: #d94d3a;}
.pun{color: #515151;}
.atv{color: #009f5d;}
.c069{color:#006699}
.gray{color:gray}
.ca70{color:#aa7700}
.c090{color:#009900}
.cf149{color:#ff1493}
.bold{font-weight: bold;}
.uline {text-decoration: underline;}
.plain{border-radius: 5px;color: #545353;border:1px solid #e1e1e8;border-radius:5px;line-height:18px;position:relative;padding-left:45px;background:#f7f7f9;}
.plain .leftNum{line-height:18px;position:absolute;left:0;top:0;bottom:0;border-right:1px solid #ececf0;background:#fbfbfc;width:30px;padding-top:5px}
.plain .leftNum span{line-height:18px;display:block;text-align:right;padding:0 6px 0 0px;color:#afafaf;font-size:12px;font-weight:700}
.plain .prettyprint {background: #f7f7f9;}

.generalScheme{position:absolute;width: 590px;margin-left:200px;margin-top:96px;}
.generalScheme dt{text-align:center;padding:23px 0;font-weight:bold;font-size:20px}
.generalScheme dd.on{font-weight:bold}
.generalScheme dd{float:left;width: 28%;border:1px dashed #efefef;margin: 0 10px;padding:5px;height:80px;background-color:#fcfcfc;font-size:15px;}





